            <kind>1</kind>
            <string>///Move player
if !dialog.show
{
    for(var i = 0; i &lt; 4; i++)
    {
        if keyboard_check(ord(keys[i]))
        {
            var dir = i * 90
            var xx = lengthdir_x(sp, dir)
            var yy = lengthdir_y(sp, dir)
            
            if place_empty(x + xx, y + yy)
            {
                x += xx
                y += yy
            }
            
            //continue animaton
            sprite_index = sprite_go[i]
            image_speed = img_sp
        }
    }
}

//break animation
if x == xprevious &amp;&amp; y = yprevious
{
    image_speed = 0
    image_index = 0
}
</string>
